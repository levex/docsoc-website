.sponsor-row {
  width: 100%;
  height: 100px;
  margin-top: 30px;
  margin-bottom: 30px;
}

.sponsor-table {
  display: table
}

.sponsor-image-cell, .sponsor-info-cell {
  vertical-align: top;
}

.sponsor-image-cell {
  float: left;
  padding: 5px 10px 20px 10px;
  text-align: center;
  width: 220px;
  height: 100px;
}

.sponsor-info-cell {
  float: left;
  padding: 0px 10px 20px 15px;
  width: 450px;
}

.sponsor-image {
  max-height: 80px
}

.sponsor-name {
  font-weight: bold;
  font-size: 24px;
}

.sponsor-info {
}

.sponsor-attention {
  font-weight: bold;
  text-align: center;
}

.clearboth {
  clear: both;
  display: block;
}
